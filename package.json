{
  "name": "nestjs-multi-tenant-starter-project-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "NODE_ENV=test jest --config ./test/jest-integration.json --runInBand",
    "test:integration:setup": "bash scripts/setup-test-db.sh",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docs:generate": "compodoc -p tsconfig.json -s",
    "docs:build": "compodoc -p tsconfig.json -d docs/compodoc",
    "docs:serve": "compodoc -p tsconfig.json -s -w",
    "docs:full": "npm run docs:build",
    "docs:validate": "bash scripts/validate-docs.sh",
    "docs:lint": "markdownlint docs/**/*.md --ignore docs/compodoc",
    "docs:links": "find docs -name '*.md' -not -path 'docs/compodoc/*' -exec markdown-link-check {} \\;",
    "docs:api:generate": "bash scripts/generate-api-docs.sh"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.901.0",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.6",
    "@prisma/client": "^6.16.3",
    "@react-email/components": "^0.5.6",
    "@react-email/render": "^1.3.2",
    "@react-email/tailwind": "^1.2.2",
    "@types/nodemailer": "^7.0.2",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.61.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "google-auth-library": "^10.4.0",
    "googleapis": "^161.0.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.1",
    "nodemailer": "^7.0.9",
    "onesignal-node": "^3.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prom-client": "^15.1.3",
    "react": "^19.2.0",
    "reflect-metadata": "^0.2.2",
    "resend": "^6.1.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^5.10.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.25",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^19.2.2",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "markdown-link-check": "^3.13.7",
    "markdownlint-cli": "^0.45.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.4.2",
    "prisma": "^6.16.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.(ts|tsx)$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest",
      "^.+\\.(t|j)sx$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s",
      "**/*.(t|j)sx"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
